{
  "version": 2,
  "name": "modern-stack-resume-graphql",
  "env": {
    "IS_NOW": "true",
    "FIREBASE_PROJECT_ID": "@firebase-project-id",
    "FIREBASE_KEY_ID": "@firebase-key-id",
    "FIREBASE_PRIVATE_KEY": "@firebase-private-key",
    "FIREBASE_CLIENT_EMAIL": "@firebase-client-email",
    "FIREBASE_CLIENT_ID": "@firebse-client-id",
    "FIREBASE_CERT_URL": "@firebase-cert-email",
    "REDIS_PASSWORD": "@redis-password",
    "REDIS_DB_NAME": "@redis-db-name",
    "REDIS_URL": "@redis-url"
  },
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@now/node@canary"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}

